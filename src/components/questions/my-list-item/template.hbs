<div class="my-list-item"
     data-component="my-list-item"
     data-questionId="{{questionId}}">
  <div class="my-list-item__navigation-container"
       data-element="my-list-item__navigation-container">
    <div class="my-list-item__buttons-container">
      <div class="my-list-item__button-load-container"
           data-element="my-list-item__button-load-container">
      </div>
      <div class="my-list-item__button-details-container"
           data-element="my-list-item__button-details-container">
      </div>
    </div>
  </div>

  <div class="my-list-item__content"
       data-element="my-list-item__content">
    <p class="my-list-item__theme">
      {{session}}
    </p>
    <p class="my-list-item__status">
      <span>Статус: </span>{{status}}
    </p>
    <p class="my-list-item__question"
       data-element="my-list-item__question">
      {{question}}
    </p>
  </div>

</div>
